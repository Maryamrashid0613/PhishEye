{
  "name": "PhishEye - Real-Time Phishing Detector",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Detect phishing pages and warn users in real-time.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>",
    "http://127.0.0.1:5000/*",
    "http://localhost:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  }
}
